<Window x:Class="StrategoServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StrategoServer"
        mc:Ignorable="d"
        xmlns:c="clr-namespace:StrategoServer"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel >
        <DockPanel >
            <ListBox ItemsSource="{Binding}">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Name}" />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ListBox>
            <Separator VerticalAlignment="Center"></Separator>
            <ListView DockPanel.Dock="Right">
                <Label Content="hello" ></Label>
            </ListView>
        </DockPanel>
    </DockPanel>
</Window>
